{
  "name": "all-in-one-product-reviews",
  "version": "1.0.0",
  "description": "Product Review Application",
  "main": "server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "start": "nodemon ./server/dev/server --exec babel-node",
    "clean:dist": "rd /s /q dist",
