    "serve": "node ./dist/server",
    "serve:dist": "npm run build:dist && npm run serve"
  },
  "author": "Aaron Leyesa",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "mocha": "^5.2.0"
  }
}
